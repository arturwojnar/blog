{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/talks",
      "destination": "/talks.html"
    },
    {
      "source": "/trainings",
      "destination": "/trainings.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/api/index"
    }
  ]
}